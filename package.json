{
  "name": "nordkaliber-netlify",
  "version": "1.0.0",
  "description": "Nordkaliber website with Stripe integration for Netlify",
  "main": "api/index.js",
  "scripts": {
    "build": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build:prod": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "start": "node api/index.js"
  },
  "keywords": [
    "nordkaliber",
    "stripe",
    "ecommerce",
    "netlify"
  ],
  "author": "Vittoriobre",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.5",
    "stripe": "^14.10.0"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  }
}