{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/design",
      "dest": "/public/design/design.html"
    },
    {
      "src": "/checkout",
      "dest": "/public/checkout.html"
    },
    {
      "src": "/success",
      "dest": "/public/success.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
} 