{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/design/design.html",
      "dest": "/design",
      "status": 301
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/design",
      "dest": "/public/design/design.html"
    },
    {
      "src": "/checkout",
      "dest": "/public/checkout.html"
    },
    {
      "src": "/success",
      "dest": "/public/success.html"
    },
    {
      "src": "/policies",
      "dest": "/public/policies.html"
    },
    {
      "src": "/test-email",
      "dest": "/public/test-email.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
} 