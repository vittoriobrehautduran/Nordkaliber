{
  "version": 2,
  "cleanUrls": true,
  "buildCommand": "echo 'No build step needed'",
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/design/design.html",
      "dest": "/design",
      "status": 301
    },
    {
      "src": "/design/designpage",
      "dest": "/designpage.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/design",
      "dest": "/designpage.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/checkout",
      "dest": "/checkout.html"
    },
    {
      "src": "/success",
      "dest": "/success.html"
    },
    {
      "src": "/policies",
      "dest": "/policies.html"
    },
    {
      "src": "/test-email",
      "dest": "/test-email.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
} 